regcode={"R0": "000", "R1": "001", "R2": "010", "R3": "011", "R4": "100", "R5": "101", "R6": "110", "FLAG": "111"}

def op00000(cmnd):
    return ("00000"+"00"+f"{regcode[cmnd[1]]}"+f"{regcode[cmnd[2]]}"+f"{regcode[cmnd[3]]}")

